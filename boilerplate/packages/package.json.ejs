{
  "name": "@<%= props.organization %>/<%= props.packageName %>",
  "version": "1.0.0",
  "description": "Technology Studio - <%= props.fullname %>",
  "main": "index.js",
  "typings": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "<%= props.repository %><%= props.packageName %>.git"
  },
  "author": {
    "name": "Technology Studio",
    "email": "npm@technologystudio.sk",
    "url": "<%= props.repository %>"
  },
  "license": "<%= props.license %>",
  "private": false,<% if (props.registry && props.access === 'private') { %>
  "publishConfig": {
    "registry": "<%= props.registry %>"
  },
  "scripts": {
    "republish": "npm unpublish $(cat package.json | jq -r '.name')@$(cat package.json | jq -r '.version') && yarn publish --new-version $(cat package.json | jq -r '.version')"
  },<% } else { %>
  "scripts": {
  },<% } %>
  "dependencies": {
  },
  "peerDependencies": {
  }
}
